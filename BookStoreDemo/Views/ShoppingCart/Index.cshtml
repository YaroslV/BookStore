@model BookStoreDemo.ViewModel.CartViewModel


@{
    ViewBag.Title = "Selected Books";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>
            Book Name
        </th>
        <th>
            Quantity
        </th>
    </tr>


    @foreach (var item in Model.CartItems) { 
        <tr>
            <td>@Html.DisplayFor(it => item.BookToBuy.BookName)</td>
        </tr>
    <tr>
        <td>@Html.DisplayFor(it => item.Count)</td>
    </tr>

    }

</table>

